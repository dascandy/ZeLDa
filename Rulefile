

src/(.*)\.cpp => obj/src/\1.o obj/src/\1.d
  g++ -c -o $@ $^ -MMD -std=c++14 -g
(.*)\.(?:c|cpp) => obj/\1.o obj/\1.d
  g++ -c -o $@ $^ -MMD -std=c++14 -m32 -ffunction-sections -fdata-sections

obj/src/.*\.o => bin/zelda
  g++ -o $@ $^ -g

obj/test/.*\.o /usr/lib/x86_64-linux-gnu/crt1.o /usr/lib/x86_64-linux-gnu/libc.a [bin/zelda] => bin/test/hello.elf
  bin/zelda -o $@ $^

bin/test/hello.elf => all

depfiles obj/.*\.d
generated obj/.*

